{
    "name": "botranktir",
    "version": "2.0.0",
    "description": "Discord bot that manages roles after reacting to a message",
    "main": "build/src/app.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "prod": "tsc && pm2 start pm2/pm2.json",
        "reload": "tsc && pm2 reload pm2/pm2.json",
        "dev": "pm2-dev start pm2/pm2-dev.json",
        "kill": "pm2 delete pm2/pm2.json",
        "build": "tsc",
        "eslint": "eslint --ext .ts,.js src/"
    },
    "author": "TruckersMP",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/TruckersMP/botranktir"
    },
    "dependencies": {
        "discord.js": "^12.2.0",
        "discord.js-commando": "github:discordjs/Commando",
        "knex": "^0.21.1",
        "mysql": "^2.18.1",
        "objection": "^2.1.0",
        "pm2": "^4.4.0"
    },
    "devDependencies": {
        "@typescript-eslint/parser": "^2.33.0",
        "@types/node": "^13.13.5",
        "eslint": "^7.0.0",
        "ts-node": "^8.10.1",
        "typescript": "^3.8.3"
    }
}
